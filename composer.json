{
    "name": "snebes/notifications",
    "description": "The Notification component provides all the tools needed to build a complete notification system.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Nebes",
            "email": "steve@nebes.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "SN\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.3",
        "doctrine/orm": "^2.5",
        "doctrine/collections": "^1.0",
        "symfony/event-dispatcher": "^3.4|^4.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.6",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^7.0|^8.0",
        "symfony/config": "^3.4|^4.2",
        "symfony/framework-bundle": "^3.4|^4.2",
        "symfony/http-kernel": "^3.4|^4.2",
        "symfony/phpunit-bridge": "^3.4|^4.2",
        "symfony/twig-bundle": "^3.4|^4.2",
        "symfony/yaml": "^3.4|^4.2"
    }
}
